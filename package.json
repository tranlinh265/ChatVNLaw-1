  "scripts": {
    "build-css-common": "node-sass-chokidar src/assets/styles/common -o src/assets/styles/common",
    "build-css-dashboard": "node-sass-chokidar src/assets/styles/dashboard/style.scss -o src/assets/styles/dashboard/",
    "watch-css": "npm run build-css-common && npm run build-css-dashboard && node-sass-chokidar src/assets/styles/common -o src/assets/styles/common --watch --recursive",
    "start-js": "react-scripts start",
    "start": "BROWSER=none npm-run-all -p watch-css start-js",
    "build": "npm run build-css-common && npm run build-css-dashboard && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
